{
  "name": "hadron-webapp",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src/. --ext js --ext jsx --fix",
    "test": "jest --testTimeout=10000",
    "test:watch": "jest --watch --testTimeout=10000",
    "start": "webpack-dev-server --mode development --devtool source-map --hot",
    "copy-static": "mkdir -p dist && cp -a assets dist/",
    "prebuild": "npm install && npm run copy-static",
    "build": "webpack --mode production",
    "watch": "webpack-dev-server --mode development --devtool source-map --hot --watch"
  },
  "dependencies": {
    "date-fns": "^2.19.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-table": "^7.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^13.0.15",
    "babel-loader": "^8.2.2",
    "babel-plugin-jsx-control-statements": "^4.1.0",
    "css-loader": "^5.2.0",
    "eslint": "^7.22.0",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-react": "^7.23.1",
    "faker": "^5.4.0",
    "file-loader": "^6.2.0",
    "history": "^5.0.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "jest-prop-type-error": "^1.1.0",
    "jest-watch-typeahead": "^0.6.1",
    "jest-when": "^3.2.1",
    "node-sass": "^5.0.0",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.3.0",
    "webpack": "^5.28.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  }
